{
    "name": "OneMark",
    "version": "1.03",
    "description": "Add-on for OneMark",
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "images/logo-16.png",
            "32": "images/logo-32.png",
            "48": "images/logo-48.png",
            "128": "images/logo-128.png"
        },
        "default_title": "Add this page to OneMark",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "notifications",
        "contextMenus",
        "https://onemark.herokuapp.com/"
    ],
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "options_page": "options.html",
    "manifest_version": 2
}